@model IEnumerable<string>

@{
    ViewBag.Title = "Names";
}

<h2>Names</h2>
@if (Model.Count() == 0)
{
    using (Html.BeginForm())
    {
        for(var i = 0; i < 3; i++) {
            <div><label>@(i + 1):</label>@Html.TextBox("names")</div>
        }
        <button type="submit">Submit</button>
    }
}
else
{
    foreach (var str in Model)
    {
        <p>@str</p>
    }
    @Html.ActionLink("Back", "Names");
}
